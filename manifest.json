    "version": "4.0.2",
    "icon": "./icon.png",
    "authors": [
        {
            "name": "MiyakoLol",
            "link": "https://github.com/MiyakoLol"
        },
        {
            "name": "captain-wangrun-cn",
            "link": "https://github.com/captain-wangrun-cn"
        }
    ],
    "platform": ["win32", "linux", "darwin"],
